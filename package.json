{
  "name": "server",
  "version": "1.0.2",
  "private": true,
  "description": "Automated Express Backend",
  "main": "src/index.js",
  "scripts": {
    "docker": "docker-compose up --wait",
    "migrator": "yarn ts-node src/shared/db/migrator.ts",
    "build": "yarn tsc --build --verbose",
    "dev": "yarn install && yarn docker && yarn build && yarn concurrently",
    "start": "yarn run dev",
    "concurrently": "concurrently \"yarn tsc --watch\" \"nodemon -q dist/src/index.js\"",
    "mon": "nodemon -q dist/src/index.js",
    "test": "yarn jest",
    "clean": "rm -rf node_modules yarn.lock dist build"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^7.7.5",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "pg": "^8.8.0",
    "sequelize": "^6.24.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-jest": "^29.1.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.24.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "jest": "^29.1.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "openapi-types": "^12.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
